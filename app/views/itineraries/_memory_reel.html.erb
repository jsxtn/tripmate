<div class="memory-container"<%#fade-out"%>  >
  <h2><%=@username%>'s trip to Thailand</h2>
  <div id="carouselExample" class="carousel slide mx-3 mb-5 mt-3" data-ride="carousel">
    <div class="carousel-inner">
      <div class="carousel-item active">
      <%= image_tag "https://seoimgak.mmtcdn.com/blog/sites/default/files/images/Best%20beaches%20of%20Thailand%20are%20in%20Krabi.jpg", class:"c-img d-block w-100"%>
      </div>

      <div class="carousel-item">
      <%= image_tag "https://assets.traveltriangle.com/blog/wp-content/uploads/2018/08/Khao-Luang-Cave.jpg?tr=w-400", class:"c-img d-block w-100" %>
      </div>

      <div class="carousel-item">
      <%= image_tag "https://www.planetware.com/wpimages/2019/08/thailand-best-places-to-visit-chiang-mai.jpg", class:"c-img d-block w-100"%>
      </div>

      <div class="carousel-item ">
      <%= image_tag "https://lh5.googleusercontent.com/proxy/UTD4pe0oBPVTNHuO8bAjNFeVY3u_bVe7us2p2NxN7xA9hFlloYLbFXFoGA9pprQ5FwYQcwQ6bBaokgiP5WmYb9yNDj6l5YO50FherWGyVdCh3g2aWkHgrOJJViF0tg", class:"c-img d-block w-100"%>
      </div>

    <div class="carousel-item">
    <%= image_tag "https://media.istockphoto.com/id/1287411694/photo/chef-cooking-food-at-street-side-restaurant-in-yaowarat-road-bangkok.jpg?s=612x612&w=0&k=20&c=lu3wmk3q2Dtr1eeKzZcWORI1bW-mGri4bJtH8Sl3MKU=", class:"c-img d-block w-100"%>
      </div>


    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Next</span>
    </button>
  </div>
</div>
