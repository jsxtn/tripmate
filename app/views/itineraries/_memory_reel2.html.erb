<div class="memory-container-2 mt-2">
  <div class="mem-reel-title-2 flex align-items-center justify-content-center">
    <h2 class="text-center mt-2"><%=@username%>'s trip to Rome</h2>
  </div>

  <div id="carouselExampleControls" class="carousel slide mx-3 mb-4 mt-3" data-ride="carousel">
    <div class="carousel-inner">

      <div class="carousel-item active">
        <%= image_tag "https://t3.ftcdn.net/jpg/02/27/88/36/360_F_227883696_tvfOO6syjvdRJYZls1FvCSgUqfVAcNXL.jpg", class:"c-img d-block w-100"%>
      </div>

      <div class="carousel-item">
        <%= image_tag "https://www.italyperfect.com/blog/wp-content/uploads/2019/04/Italy-Perfect-Expert-Shares-Rome-Sightseeing-Tips-by-Italy-Perfect4.jpg", class:"c-img d-block w-100" %>
      </div>

      <div class="carousel-item">
        <%= image_tag "https://media.istockphoto.com/id/891284194/photo/pasta-pizza-and-homemade-food-arrangement-in-a-restaurant-rome.jpg?s=612x612&w=0&k=20&c=w9dYWN9kqLixeJcN4OmTP4FCSMY4uh31nrXklBqIngw=", class:"c-img d-block w-100" %>
      </div>

      <div class="carousel-item">
        <%= image_tag "https://www.planetware.com/photos-large/I/pantheon.jpg", class:"c-img d-block w-100" %>
      </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>

  <div class="print-icon-2">
    <a href="https://www.cewe.co.uk/photo-books.html?cref=om_sem_goo_-VT!152344009344!kwd-375607670306!664701601381!!c!g!19610142469_12611_x&s_kwcid=AL!6025!3!664701601381!b!!g!!crew%20photo%20book&keyword=crew%20photo%20book&gad_source=1&gclid=CjwKCAjw9cCyBhBzEiwAJTUWNcwF-L_LHNlUZuY0YrJ1h6rnkCC1ohusA840FrhgQiwQzRm_XYr5WhoCtYUQAvD_BwE" class="btn-tripmate-black share" target="_blank"><i class="fa-solid fa-print"></i></a>
  </div>
</div>
